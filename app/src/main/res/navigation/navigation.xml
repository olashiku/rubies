<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/chatFragment">
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.qucoon.rubiesnigeria.views.fragment.internal.chat_details.view.ChatDetailsFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" >
        <action
            android:id="@+id/action_chatFragment_to_contactFragment"
            app:destination="@id/contactFragment" />
    </fragment>
    <fragment
        android:id="@+id/comminityFragment"
        android:name="com.qucoon.rubiesnigeria.views.fragment.internal.community.view.ComminityFragment"
        android:label="fragment_comminity"
        tools:layout="@layout/fragment_comminity" />
    <fragment
        android:id="@+id/walletFragment"
        android:name="com.qucoon.rubiesnigeria.views.fragment.internal.wallet.view.WalletFragment"
        android:label="fragment_wallet"
        tools:layout="@layout/fragment_wallet" />
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.qucoon.rubiesnigeria.views.fragment.internal.contact.view.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" >
        <action
            android:id="@+id/action_contactFragment_to_chatScreenFragment"
            app:destination="@id/chatScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatScreenFragment"
        android:name="com.qucoon.rubiesnigeria.views.fragment.internal.chat_screen.view.ChatScreenFragment"
        android:label="fragment_chat_screen"
        tools:layout="@layout/fragment_chat_screen" />
</navigation>